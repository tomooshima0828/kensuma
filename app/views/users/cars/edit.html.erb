<% provide(:title, "車両情報編集") %>
<% provide(:btn_text, "更新") %>

<div class="card">
  <%= form_with model: @car, url: users_car_path, method: :patch, local: true do |f| %>
    <div class="card-body">
      <%= render partial: "form", locals: { f: f, car: @car} %>
    </div>
    <div class="card-footer">
      <%= f.submit yield(:btn_text), class: "btn btn-md btn-block btn-success" %>
      <%= link_to "戻る", users_car_path, class: "btn btn-md btn-block btn-default" %>
    </div>
  <% end %>
</div>
